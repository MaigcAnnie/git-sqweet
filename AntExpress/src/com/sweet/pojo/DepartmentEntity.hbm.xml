<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.sweet.pojo.DepartmentEntity" table="Department" schema="dbo" catalog="AntExpress">
        <id name="depId">
            <column name="DepId" sql-type="int"/>
        </id>
        <property name="depName">
            <column name="DepName" sql-type="nvarchar(16)" length="16" not-null="true"/>
        </property>
        <property name="depDetail">
            <column name="DepDetail" sql-type="nvarchar(256)" length="256" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>