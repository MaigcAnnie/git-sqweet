<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.sweet.pojo.CustomerEntity" table="Customer" schema="dbo" catalog="AntExpress">
        <id name="cstLogin">
            <column name="CstLogin" sql-type="varchar(16)" length="16"/>
        </id>
        <property name="cstPwd">
            <column name="CstPwd" sql-type="varchar(16)" length="16" not-null="true"/>
        </property>
        <property name="cstTel">
            <column name="CstTel" sql-type="char(11)" length="11" not-null="true"/>
        </property>
        <property name="cstAddress">
            <column name="CstAddress" sql-type="nvarchar(256)" length="256" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>