<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.sweet.pojo.SendFormEntity" table="SendForm" schema="dbo" catalog="AntExpress">
        <id name="sendId">
            <column name="SendId" sql-type="int"/>
        </id>
        <property name="senderName">
            <column name="SenderName" sql-type="nvarchar(16)" length="16" not-null="true"/>
        </property>
        <property name="senderTel">
            <column name="SenderTel" sql-type="varchar(11)" length="11" not-null="true"/>
        </property>
        <property name="sendDepartment">
            <column name="SendDepartment" sql-type="nvarchar(64)" length="64" not-null="true"/>
        </property>
        <property name="receiverName">
            <column name="ReceiverName" sql-type="nvarchar(16)" length="16" not-null="true"/>
        </property>
        <property name="receiverTel">
            <column name="ReceiverTel" sql-type="varchar(11)" length="11" not-null="true"/>
        </property>
        <property name="receiveDep">
            <column name="ReceiveDep" sql-type="nvarchar(64)" length="64" not-null="true"/>
        </property>
        <property name="sendDatetime">
            <column name="SendDatetime" sql-type="datetime" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>